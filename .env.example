# Hatchbridge Visit - Environment Variables
# Copy this file to .env and fill in your values

# Server Port (optional, defaults to 3000)
PORT=3000

# MongoDB Connection String
# For Railway: Add MongoDB plugin and use the provided MONGODB_URI
# For external: Use your MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/hatchbridge-visit?retryWrites=true&w=majority

# JWT Secret for authentication tokens
# Generate a secure random string for production
JWT_SECRET=your-secure-secret-here
